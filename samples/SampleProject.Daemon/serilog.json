{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"System": "Error",
				"Microsoft": "Error"
			}
		},
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:sszzz} [{Level:w3}] {SourceContext} {Message:lj}{NewLine}{Exception}"
				}
			}
		],
		"Enrich": [ "FromLogContext" ]
	}
}