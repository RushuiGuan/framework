<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<Description>A customizable code generator engine with built in support for C# and TypeScript code generation</Description>
		<PackageTags>code generator c# sql codegen powershell database csharp</PackageTags>
		<PackageProjectUrl></PackageProjectUrl>
		<Copyright>Copyright Â© 2019</Copyright>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.CodeAnalysis.Common" Version="4.12.0" />
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.2" />
		<PackageReference Include="Albatross.Reflection" Version="7.5.7" />
		<PackageReference Include="Albatross.Text" Version="7.5.8" />
		<PackageReference Include="Albatross.CodeAnalysis.MSBuild" Version="7.6.1" />
	</ItemGroup>
	<ItemGroup>
		<None Include="..\..\icon.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
</Project>