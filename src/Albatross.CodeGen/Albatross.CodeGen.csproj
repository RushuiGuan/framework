<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	  <TargetFramework>net5.0</TargetFramework>
    <PackageId>Albatross.CodeGen</PackageId>
    <Authors>Rushui Guan</Authors>
    <Product>Albatross Code Generator</Product>
    <Description>A customizable code generator engine with a Powershell tool that allows user to quickly setup composite generators and update the target file.</Description>
    <PackageTags>code generator c# sql codegen powershell database csharp</PackageTags>
    <PackageProjectUrl></PackageProjectUrl>
    <Copyright>Copyright Â© 2019</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/RushuiGuan/framework</RepositoryUrl>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>
      </PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Albatross\Albatross.csproj" />
  </ItemGroup>
</Project>