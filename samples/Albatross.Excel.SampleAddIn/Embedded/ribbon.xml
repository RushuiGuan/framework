<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns='http://schemas.microsoft.com/office/2006/01/customui'>
	<ribbon>
		<tabs>
			<tab id='sample_tab' label='sample'>
				<group id='group1' label='Font Properties'>
					<button id='PrintColor' label='Print Color' onAction='Btn_PrintColor'/>
					<button id='FontProperties' label='FontProperties' onAction='Btn_FontProperties'/>
					<button id='NumberFormat' label='NumberFormats' onAction='Btn_NumberFormats'/>
					<button id='Background' label='Background' onAction='Btn_Background'/>
				</group>
				<group id='group2' label='Table Demo'>
					<button id='LoadTestData' label='Load TestData' onAction='Btn_LoadTestData'/>
					<button id='LoadInstrumentData' label='Load Instrument Data' onAction='Btn_LoadInstrumentData'/>
					<button id='SavePrice' label='Save Price' onAction='Btn_SavePrice'/>
				</group>
				<group id='group3' label='Configuration'>
					<labelControl id='envLabel' getLabel='GetEnvironment' />
					<button id='ShowHelpBtn' label='Show Help' onAction='Btn_ShowHelp'/>
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>