{
	"metadata": [
		{
			"src": [
				{
					"files": [
						"authentication/albatross.authentication/*.csproj",
						"caching/albatross.caching/*.csproj",
						"caching/albatross.caching.redis/*.csproj",
						"caching/albatross.caching.memcache/*.csproj",
						"codegen/albatross.codegen/*.csproj",
						"codegen/albatross.codegen.webclient/*.csproj",
						"config/albatross.config/*.csproj",
						"dates/albatross.dates/*.csproj",
						"efcore/albatross.efcore/*.csproj",
						"efcore/albatross.efcore.changereporting/*.csproj",
						"efcore/albatross.efcore.datelevel/*.csproj",
						"efcore/albatross.efcore.postgresql/*.csproj",
						"efcore/albatross.efcore.sqlserver/*.csproj",
						"excel/albatross.excel/*.csproj",
						"excel/albatross.hosting.excel/*.csproj",
						"hosting/albatross.hosting/*.csproj",
						"hosting/albatross.hosting.test/*.csproj",
						"hosting/albatross.hosting.utility/*.csproj",
						"logging/albatross.logging/*.csproj",
						"messaging/albatross.messaging/*.csproj",
						"serialization/albatross.serialization/*.csproj",
						"albatross/albatross/*.csproj",
						"webclient/webclient/*.csproj"
					],
					"src": ".."
				}
			],
			"dest": "api",
			"filter": "filterConfig.yml",
			"includePrivateMembers": false,
			"disableGitFeatures": true,
			"disableDefaultFilter": false,
			"noRestore": false,
			"namespaceLayout": "nested",
			"memberLayout": "samePage",
			"allowCompilationErrors": false
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"articles/**.md",
					"articles/**/toc.yml",
					"toc.yml",
					"*.md"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				]
			}
		],
		"output": "_site",
		"globalMetadata":{
			"_appName": "Albatross Framework",
      		"_appTitle": "Albatross Framework Documentation"
		},
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"default",
			"modern",
			"template/material"
		],
		"postProcessors": [],
		"keepFileLink": false,
		"disableGitFeatures": true
	}
}