<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<PackageId>Albatross.CodeGen</PackageId>
		<Authors>Rushui Guan</Authors>
		<Product>Albatross Code Generator</Product>
		<Description>A customizable code generator engine with built in support for C# and TypeScript code generation</Description>
		<PackageTags>code generator c# sql codegen powershell database csharp</PackageTags>
		<PackageProjectUrl></PackageProjectUrl>
		<Copyright>Copyright Â© 2019</Copyright>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<RepositoryUrl>https://github.com/RushuiGuan/framework</RepositoryUrl>
		<Nullable>enable</Nullable>
		<PackageReadmeFile>readme.md</PackageReadmeFile>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.1" />
	</ItemGroup>
	<ItemGroup>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath>
			</PackagePath>
		</None>
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\..\reflection\Albatross.Reflection\Albatross.Reflection.csproj" />
	  <ProjectReference Include="..\..\text\Albatross.Text\Albatross.Text.csproj" />
	</ItemGroup>
	<ItemGroup>
	  <None Update="readme.md">
	    <PackagePath>\</PackagePath>
	    <Pack>True</Pack>
	  </None>
	</ItemGroup>
</Project>