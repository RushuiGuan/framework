<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<OutputType>Library</OutputType>
		<Nullable>enable</Nullable>
		<Description>Supports parsing and operations of Major, Minor, Patch, Prerelease and Metadata according to semver2.0 spec.</Description>
		<PackageTags>semver semantic version</PackageTags>
		<PackageProjectUrl>$(RepositoryProjectRoot)/devtools/Albatross.SemVer</PackageProjectUrl>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\icon.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
</Project>