<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net8.0;net7.0;net6.0</TargetFrameworks>
		<IsPackable>true</IsPackable>
		<ApplicationIcon />
		<OutputType>Library</OutputType>
		<StartupObject />
		<RepositoryUrl>https://github.com/RushuiGuan/framework</RepositoryUrl>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<Authors>Rushui Guan</Authors>
		<Description>hosting framework for test projects</Description>
		<Nullable>enable</Nullable>
		<PackageReadmeFile></PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Hosting" Version="8.0.0" />
		<PackageReference Include="Albatross" Version="6.4.6" />
		<PackageReference Include="Albatross.Authentication" Version="7.0.0" />
		<PackageReference Include="Albatross.Config" Version="6.4.1" />
		<PackageReference Include="Albatross.EFCore.SqlServer" Version="6.5.1" />
		<PackageReference Include="Albatross.Logging" Version="6.4.5" />
		<PackageReference Include="CsvHelper" Version="30.0.1" />
		<PackageReference Include="Moq" Version="4.20.70" />
		<PackageReference Include="xunit" Version="2.6.6" />
		<PackageReference Include="xunit.runner.visualstudio" Version="2.5.6">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>
	<ItemGroup>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath>
			</PackagePath>
		</None>
	</ItemGroup>
</Project>