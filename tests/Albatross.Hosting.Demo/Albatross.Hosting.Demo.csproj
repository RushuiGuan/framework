<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<StartupObject></StartupObject>
		<AssemblyName>Albatross.Hosting.Sample</AssemblyName>
		<Version>1.6.0-alpha</Version>
		<RootNamespace>Albatross.Hosting.Sample</RootNamespace>
	</PropertyGroup>
	<PropertyGroup>
		<AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
		<OutputType>Exe</OutputType>
	</PropertyGroup>
	
	<ItemGroup>
		<ProjectReference Include="..\..\src\Albatross.Hosting\Albatross.Hosting.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Folder Include="Properties\" />
		<Folder Include="wwwroot\" />
	</ItemGroup>
	<ItemGroup>
		<Content Update="hostsettings.json">
			<CopyToOutputDirectory>Never</CopyToOutputDirectory>
		</Content>
		<Content Update="serilog.json">
		  <CopyToOutputDirectory>Never</CopyToOutputDirectory>
		</Content>
	</ItemGroup>
</Project>