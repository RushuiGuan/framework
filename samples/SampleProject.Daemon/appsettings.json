{
    "program": {
        "app": "SampleProjectDaemon",
        "serviceManager": "windows"
    },
    "endpoints": {
        "messaging-server": "tcp://localhost:23000"
    },
    "messaging": {
        "routerServer": {
            "diskStorage": {
                "workingDirectory": "c:\\app\\messaging\\logs"
            },
            "logCatchUpPeriod": 60,
            "maintainConnection": true
        }
    }
}
