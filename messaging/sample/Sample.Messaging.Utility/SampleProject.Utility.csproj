<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
	</PropertyGroup>
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<Nullable>enable</Nullable>
		<AssemblyName>sample-project</AssemblyName>
		<EnableNETAnalyzers>False</EnableNETAnalyzers>
	</PropertyGroup>
	<ItemGroup>
		<Folder Include="Properties\" />
	</ItemGroup>
	<ItemGroup>
	  <PackageReference Include="Albatross.Hosting.Utility" Version="6.4.0" />
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\..\Albatross.Messaging\Albatross.Messaging.csproj" />
	  <ProjectReference Include="..\Sample.Messaging.Commands\SampleProject.Commands.csproj" />
	  <ProjectReference Include="..\Sample.Messaging.Proxy\SampleProject.Proxy.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Content Update="serilog.staging.json">
			<CopyToOutputDirectory>Never</CopyToOutputDirectory>
		</Content>
		<Content Update="serilog.json">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>
</Project>