<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<OutputType>Library</OutputType>
		<Nullable>enable</Nullable>
		<Description>Semantic 2.0 Version Parsing and Operation Implementation using DotNet</Description>
		<PackageTags>semver semantic version</PackageTags>
		<PackageProjectUrl>$(RepositoryProjectRoot)/devtools/Albatross.SemVer</PackageProjectUrl>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\..\icon.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
</Project>