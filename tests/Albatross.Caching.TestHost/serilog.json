{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"System": "Error",
				"Microsoft": "Error"
			}
		},
		"WriteTo": {
			"ConsoleSink": {
				"Name": "Console",
				"Args": {
					"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:sszzz} [{Level:w3}] {Properties:j} {Message:lj}{NewLine}{Exception}"
				}
			},
			"FileSink": {
				"Name": "File",
				"Args": {
					"path": "%LogDirectory%\\Caching.TestHost\\Caching.log",
					"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:sszzz} [{Level:w3}] {Properties:j} {Message:lj}{NewLine}{Exception}",
					"rollingInterval": "Day"
				}
			}
		},
		"Enrich": [ "FromLogContext", "WithThreadId", "WithMachineName" ]
	}
}