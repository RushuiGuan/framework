<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>FrameworkUtil</AssemblyName>
    <StartupObject>Albatross.Framework.Utility.Program</StartupObject>
    <Version>1.6.0-alpha</Version>
    <IsPackable>false</IsPackable>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="serilog.json" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="serilog.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Albatross.Hosting.Utility\Albatross.Hosting.Utility.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>