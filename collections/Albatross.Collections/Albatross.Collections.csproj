<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<LangVersion>Latest</LangVersion>
		<Nullable>enable</Nullable>

		<PackageLicenseFile>LICENSE</PackageLicenseFile>
        <Authors>Rushui Guan</Authors>
        <Description>This assembly is a catch all utility assembly.  It exists because the different utilities within are not big enough to have its own assembly.</Description>
        <RepositoryUrl>https://github.com/RushuiGuan/framework</RepositoryUrl>
        <PackageReadmeFile>readme.md</PackageReadmeFile>
		<IncludeSymbols>True</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.0" />
    </ItemGroup>
    <ItemGroup>
        <None Include="..\..\LICENSE">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
    </ItemGroup>
</Project>